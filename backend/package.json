    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.12",
    "@types/nodemailer": "^6.4.17",
    "nodemon": "^3.1.9",
